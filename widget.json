{
  "html": "index.html",
  "js": "js/clock.js",
  "param": [{
    "name": "type",
    "default": "numeric*|analogic"
  }, {
    "name": "display_date",
    "type": "boolean",
    "default": false
  }, {
    "name": "border",
    "default": "none*|small|large"
  }, {
    "name": "border_radius",
    "default": "none*|light|heavy|rounded"
  }, {
    "name": "font_family",
    "default": "sans-serif|serif*|fantasy|cursive|monospace|arial"
  }, {
    "name": "font_size",
    "default": "full|large*|medium|small"
  }, {
    "name": "text_color",
    "type": "color",
    "default": "#000000"
  }, {
    "name": "bg_color",
    "type": "color",
    "default": "#ffffff"
  }, {
    "name": "bg_color_alpha",
    "default": "{\"range\":{\"value\":1,\"min\":0,\"max\":1,\"step\":0.01}}"
  }, {
    "name": "fixe_ratio",
    "type": "boolean",
    "default": false
  }]
}
