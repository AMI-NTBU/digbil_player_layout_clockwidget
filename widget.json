{
  "html": "index.html",
  "js": "js/clock.js",
  "param": [{
    "name": "type",
    "default": "numeric|analogic"
  },{
  	"name": "display-date",
  	"type": "boolean"
  },{
    "name": "border-radius",
    "default": "none*|light|heavy|rounded"
  }, {
    "name": "font-family",
    "default": "sans-serif|serif*|fantasy|cursive|monospace|arial"
  }, {
    "name": "font-size",
    "default": "full|large*|medium|small"
  }, {
    "name": "text_color",
    "type": "color",
    "default": "#000000"
  }, {
    "name": "bg_color",
    "type": "color",
    "default": "#ffffff"
  }, {
    "name": "bg_color.alpha",
    "default": "{\"range\":{\"value\":1,\"min\":0,\"max\":1,\"step\":0.01}}"
  }]
}